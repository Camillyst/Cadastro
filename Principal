package projar;

import java.util.ArrayList;
import java.util.Scanner;

public class Principal {

	public static void main(String[] args) {
		
	Cadastro cad = new Cadastro();
	
	cad.setNome("Camilly");
	cad.setCategoria("Doméstica");
	cad.setDescricao("Atuo na área há 15 anos.");
	cad.mostra();
	cad.removeElemento(0);
	cad.mostra();
	cad.alterarElemento(0, "Allan");
	cad.mostra();
}
}
	
